<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="#AAb6bcc3" />

            <corners
                android:bottomLeftRadius="5dip"
                android:bottomRightRadius="5dip"
                android:topLeftRadius="5dip"
                android:topRightRadius="5dip" />
        </shape>
    </item>
    <item
        android:bottom="1dip"
        android:left="1dip"
        android:right="1dip"
        android:top="1dip">
        <shape>
            <gradient
                android:angle="90"
                android:centerX="0.5"
                android:centerY="0.5"
                android:endColor="#AAf3f3f3"
                android:startColor="#AAd6d6d6"
                android:type="linear" />
            <corners
                android:bottomLeftRadius="5dip"
                android:bottomRightRadius="5dip"
                android:topLeftRadius="5dip"
                android:topRightRadius="5dip" />
        </shape>
    </item>

</layer-list>